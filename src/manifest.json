{
  "manifest_version": 2,
  "name": "Chrome Extension Update Test",
  "description": "testing updates and stuff",
  "version": "1.0.3",
  "permissions": [
    "notifications",
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ],
  "web_accessible_resources": []
}
